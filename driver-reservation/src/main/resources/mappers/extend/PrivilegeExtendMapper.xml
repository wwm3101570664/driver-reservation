<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.apps.dao.extend.PrivilegeExtendMapper">
<select id="slelectByRoleId" resultMap="com.briup.apps.dao.PrivilegeMapper.BaseResultMap">
    select p.*
    from role r,privilege p
    where r.id = p.role_id
    and r.id = #{id,jdbcType=BIGINT}
  </select>

</mapper>